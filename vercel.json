{
  "version": 2,
  "name": "real-time-webcam-vision-language-captioning",
  "build": {
    "env": {
      "NODE_VERSION": "18"
    }
  },
  "builds": [
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    },
    {
      "src": "/",
      "dest": "/frontend/index.html"
    }
  ]
}